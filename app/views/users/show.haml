- provide(:title, @user.name)
.container-fluid
  .row-fluid
    %aside.col-sm-4
      %section
        %h1
          = gravatar_for @user
          = @user.name
      -if @lists.any?
        = render 'tabs', locals: { lists: @lists, all_movies: @all_movies }
      = render 'new_list'
    .col-sm-8
      = render 'lists_content', locals: { lists: @lists, all_movies: @all_movies }
